{{define "title"}}Pintxo #{{.Pintxo.ID}}{{end}}

{{define "main"}}
    {{with .Pintxo}}
    <div class='contest'>
        <div class='metadata'>
            <strong>{{.Title}}</strong>
            <span>#{{.ID}}</span>
        </div>
        <pre><code>{{.Descr}}</code></pre>
        <div class='metadata'>
            <time>Created: {{.Created}}</time>
        </div>
    </div>
    {{end}}
{{end}}